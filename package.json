{
    "name": "filament",
    "version": "0.16.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:declarativ/filament.git"
    },
    "dependencies": {
        "montage": "git+ssh://git@github.com:montagejs/montage.git#d14af425130c949e266b02c3db31e706b6c5a066",
        "matte": "git+ssh://git@github.com:montagejs/matte.git#ed593caeed63dc337e3ceff7d70f144f79dc1697",
        "palette": "git+ssh://git@github.com:declarativ/palette.git#9b6735c62a334be808214e4b6b9bc048ffd32ac8",
        "flow-editor": "git+ssh://git@github.com:declarativ/flow-editor.git#v0.14.0",
        "minit": "git+ssh://git@github.com:montagejs/minit.git#9018fe701144de0b0e19c3124621d8c54fc6f21b",
        "mousse": "0.1.2",
        "q": "0.9.7",
        "q-connection": "0.5.5",
        "q-io": "1.10.0",
        "mop": "0.13.2",
        "mr": "0.13.4",
        "npm": "1.3.10",
        "underscore": "1.4.4",
        "semver": "2.0.10",
        "sqlite3": "2.1.17"
    },
    "devDependencies": {
        "jasmine-node": "1.2.2",
        "sandboxed-module": "0.1.4",
        "montage-testing": "0.2.0",
        "jshint": "2.1.9",
        "glTF-webgl-viewer": "git+ssh://git@github.com:fabrobinet/glTF-webgl-viewer.git#df9b068db6bea24e5518058578c23bcf90f474a4"
    },
    "mappings": {
        "filament-extension": {
            "name": "filament-extension",
            "location": "filament-extension"
        },
        "filament-asset-compiler": {
            "name": "filament-asset-compiler",
            "location": "filament-asset-compiler"
        }
    },
    "scripts": {
        "test": "node test/run.js",
        "lint": "./node_modules/.bin/jshint --verbose .",
        "lumieres": "node test/functional/run.js",

        "stage-deps": "git add --update node_modules && git add node_modules",
        "package-deps": "npm install && npm run stage-deps",
        "package-deps-clean": "rm -rf node_modules && npm run package-deps",
        "package-deps-update": "npm update && npm run stage-deps"
    },
    "manifest": true
}

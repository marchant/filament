{
    "name": "filament",
    "version": "0.26.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:declarativ/filament.git"
    },
    "dependencies": {
        "montage": "git+ssh://git@github.com:montagejs/montage.git#a7c42300dcc29ae10773dc7cc1f041885f015339",
        "matte": "git+ssh://git@github.com:montagejs/matte.git#fbf210815f4059a373f1301d658dba94b5730945",
        "palette": "git+ssh://git@github.com:declarativ/palette.git#0ca5f12ae0a928a3f2c4a448481cc500cd6d7f73",
        "flow-editor": "git+ssh://git@github.com:declarativ/flow-editor.git#a8b0f06e087175cab0bfb9769645f45c69e5631f",
        "minit": "git+ssh://git@github.com:montagejs/minit.git#v0.5.3",
        "mousse": "0.3.0",
        "q": "0.9.7",
        "q-connection": "0.6.1",
        "q-io": "1.10.0",
        "mjs-volume": "git+ssh://git@github.com:fabrobinet/mjs-volume.git#1435ff07ac6f8e8e9a42624a3f4414b2d44238fa",
        "mop": "0.13.2",
        "mr": "0.13.4",
        "npm": "1.3.10",
        "underscore": "1.4.4",
        "semver": "2.0.10"
    },
    "devDependencies": {
        "jasmine-node": "1.2.2",
        "sandboxed-module": "0.1.4",
        "montage-testing": "git+ssh://git@github.com:montagejs/montage-testing.git#86e0c6181e9694bca128bfa6b6da4333668619e1",
        "jshint": "2.1.9",
        "watchr": "2.3.3",
        "glTF-webgl-viewer": "git+ssh://git@github.com:fabrobinet/glTF-webgl-viewer.git#df9b068db6bea24e5518058578c23bcf90f474a4"
    },
    "mappings": {
        "filament-extension": {
            "name": "filament-extension",
            "location": "filament-extension"
        },
        "filament-asset-compiler": {
            "name": "filament-asset-compiler",
            "location": "filament-asset-compiler"
        }
    },
    "scripts": {
        "test": "node test/run.js",
        "lint": "./node_modules/.bin/jshint --verbose .",
        "lumieres": "node test/functional/run.js",

        "stage-deps": "git add --update node_modules && git add node_modules",
        "package-deps": "npm install && npm run stage-deps",
        "package-deps-clean": "rm -rf node_modules && npm run package-deps",
        "package-deps-update": "npm update && npm run stage-deps"
    },
    "manifest": true
}

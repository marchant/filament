{
    "name": "filament",
    "version": "0.28.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:declarativ/filament.git"
    },
    "dependencies": {
        "montage": "git+ssh://git@github.com:montagejs/montage.git#d6eaf872992e5e9cafbddaf8a933e866c984a483",
        "matte": "git+ssh://git@github.com:montagejs/matte.git#2bf2a53d5e622ec6aefc298507c03c0ec113d8dc",
        "palette": "git+ssh://git@github.com:declarativ/palette.git#a6f84f91198dfa116f74e6f22112c6e3b2561019",
        "flow-editor": "git+ssh://git@github.com:declarativ/flow-editor.git#99bb83cf5901b8b0546ee345d879758010755225",
        "minit": "git+ssh://git@github.com:montagejs/minit.git#60425519921b36c2d9722aec7343044142f5261e",
        "mousse": "0.3.0",
        "q": "0.9.7",
        "q-connection": "0.6.1",
        "q-io": "1.10.0",
        "qr-encode": "0.1.0",
        "mjs-volume": "git+ssh://git@github.com:fabrobinet/mjs-volume.git#4c1f7706088477dc858dc80fd70f172267c4d867",
        "mop": "0.13.2",
        "mr": "0.13.4",
        "npm": "1.3.10",
        "underscore": "1.4.4",
        "semver": "2.0.10"
    },
    "devDependencies": {
        "jasmine-node": "1.2.2",
        "sandboxed-module": "0.1.4",
        "montage-testing": "git+ssh://git@github.com:montagejs/montage-testing.git#86e0c6181e9694bca128bfa6b6da4333668619e1",
        "jshint": "2.1.9",
        "watchr": "2.3.3",
        "glTF-webgl-viewer": "git+ssh://git@github.com:fabrobinet/glTF-webgl-viewer.git#df9b068db6bea24e5518058578c23bcf90f474a4"
    },
    "mappings": {
        "filament-extension": {
            "name": "filament-extension",
            "location": "filament-extension"
        },
        "filament-asset-compiler": {
            "name": "filament-asset-compiler",
            "location": "filament-asset-compiler"
        }
    },
    "scripts": {
        "test": "node test/run.js",
        "lint": "./node_modules/.bin/jshint --verbose .",
        "lumieres": "node test/functional/run.js",

        "stage-deps": "git add --update node_modules && git add node_modules",
        "package-deps": "npm install && npm run stage-deps",
        "package-deps-clean": "rm -rf node_modules && npm run package-deps",
        "package-deps-update": "npm update && npm run stage-deps"
    },
    "manifest": true
}

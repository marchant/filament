{
    "name": "filament",
    "version": "0.28.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:declarativ/filament.git"
    },
    "dependencies": {
        "montage": "git+ssh://git@github.com:montagejs/montage.git#36904c2d69f43eb288b773bc25204c375129cd2f",
        "matte": "git+ssh://git@github.com:montagejs/matte.git#f5826241231011ee3c6599ad147a7c33053d9928",
        "palette": "git+ssh://git@github.com:montagestudio/palette#master",
        "flow-editor": "git+ssh://git@github.com:montagestudio/flow-editor.git#4a33a664fe39e7a8f7e7977bfe8f32f3db29c25d",
        "minit": "git+ssh://git@github.com:montagejs/minit.git#60425519921b36c2d9722aec7343044142f5261e",
        "bluebird": "~3.3.1",
        "q": "0.9.7",
        "q-connection": "0.6.1",
        "q-io": "1.10.0",
        "qr-encode": "0.1.0",
        "mjs-volume": "git+ssh://git@github.com:montagestudio/mjs-volume.git#93d2e1b0daa11e57f86421b771fd03923e8f0e60",
        "mop": "0.15.2",
        "mr": "0.15.7",
        "npm": "1.3.10",
        "underscore": "1.4.4",
        "semver": "2.0.10",
        "esprima": "^1.2.2"
    },
    "devDependencies": {
        "jasmine-node": "1.2.2",
        "sandboxed-module": "0.1.4",
        "montage-testing": "git+ssh://git@github.com:montagejs/montage-testing.git#9eca56fcec3051de6ba0b24e397e309976c14c29",
        "jshint": "2.1.9",
        "watchr": "2.3.3",
        "glTF-webgl-viewer": "./node_modules/glTF-webgl-viewer"
    },
    "mappings": {
        "filament-extension": {
            "name": "filament-extension",
            "location": "filament-extension"
        },
        "filament-asset-compiler": {
            "name": "filament-asset-compiler",
            "location": "filament-asset-compiler"
        }
    },
    "scripts": {
        "test": "node test/run.js",
        "lint": "./node_modules/.bin/jshint --verbose .",
        "lumieres": "node test/functional/run.js",

        "stage-deps": "git add --update node_modules && git add node_modules",
        "package-deps": "npm install && npm run stage-deps",
        "package-deps-clean": "rm -rf node_modules && npm run package-deps",
        "package-deps-update": "npm update && npm run stage-deps"
    },
    "manifest": true
}
